import json
import requests

class reference:
    @classmethod
    def GetHttpText(cls,path):
        return requests.get(path).text
    @classmethod
    def GetFile(cls,owner,repo,path):
        response = json.loads(cls.GetHttpText(f"https://api.github.com/repos/{owner}/{repo}/contents/{path}"))
        download_url = response["download_url"]
        result = cls.GetHttpText(download_url)
        return result
    @classmethod
    def RunScript(cls,contents):
        exec(contents, globals(), locals())

exec(reference.GetFile("jesusjorge","pysite","index.py"), globals(), locals())
