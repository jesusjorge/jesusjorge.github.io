import json
import requests

class reference:
    @classmethod
    def GetHttpText(cls,path):
        return requests.get(path).text
    @classmethod
    def GithubFile(cls,owner,repo,path):
        result = cls.GetHttpText(f"https://raw.githubusercontent.com/{owner}/{repo}/refs/heads/main/{path}")
        return result

exec(reference.GithubFile("jesusjorge","pysite","index.py"), globals(), locals())
